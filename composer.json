{
    "name": "Geoslim/laravel-er-diagram-generator",
    "description": "Generate ER diagrams from your Laravel models.",
    "keywords": [
        "Geoslim",
        "laravel-er-diagram-generator"
    ],
    "homepage": "https://github.com/Geoslim/laravel-er-diagram-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Ikponmwosa George Edomwande",
            "email": "iedomwande@yahoo.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.3",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "doctrine/dbal": "~2.3|^3.3|^4.0.2",
        "phpdocumentor/graphviz": "^1.0",
        "nikic/php-parser": "^2.0|^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "orchestra/testbench": "~3.5|~3.6|~3.7|~3.8|^4.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^7.0| ^8.0|^9.5.10|^10.5",
        "spatie/phpunit-snapshot-assertions": "^1.3|^4.2"
    },
    "autoload": {
        "psr-4": {
            "Geoslim\\ErdGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geoslim\\ErdGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Geoslim\\ErdGenerator\\ErdGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
